                        required="true"
                    />
                    <RadioButtonGroup
                        id="radioGroup"
                        columns="2"
                        selectedIndex="0"
                    >
                        <buttons>
                            <RadioButton
                                id="_IDGenRadioButton1"
                                text="Yes"
                            />
                            <RadioButton
                                id="_IDGenRadioButton2"
                                text="No"
                            />
                        </buttons>
                    </RadioButtonGroup>
 
                    <Label
                        id="_IDGenLabel3"
                        text="Supplier Spend Type"
                        required="true"
                    />
                    <Select
                        id="supplierSpendType"
                        selectedKey=""
                    >
                        <items>
                            <core:Item
                                id="_IDGenItem"
